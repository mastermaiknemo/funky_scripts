sqlite3 -list History "select url from urls;" |  grep google | grep q= | sed "s/.*&q=//g"  | sed "s/.*#q=//g" | sed "s/.*?q=//g" | sed "s/\&.*//g" |sed "s/\%../\ /g" | sed s/\+/\ /g | sed "s/&oq.*//g" | sed "s/&esrc=.*//p" |sed "s/http.*//g" | sed -n '/ / p' > ~/googlesearch && kumo  --input "file:///home/widzard/googlesearch" --output "/home/USERNAME/googlesearch.png" --word-count 20 && display ~/googlesearch.png
